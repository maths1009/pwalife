{
  "name": "pwa-life",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "packageManager": "bun@1.1.45",
  "engines-annotation": "Node 23.6.0 or later is required",
  "engines": {
    "node": ">=23.6.0"
  },
  "scripts": {
    "preview": "vite preview",
    "clean": "rimraf build dist coverage playwright-report",
    "dev": "vite",
    "build": "bun run clean && tsc -b && rimraf vite.config.js && vite build",
    "test:unit": "bun test ./tests/*.test.ts",
    "test:unit:watch": "bun test --watch ./tests/*.test.ts",
    "test:e2e": "rimraf playwright-report && playwright test ./e2e/*.spec.ts",
    "biome:start": "biome start",
    "biome:stop": "biome stop",
    "biome:fix": "biome check --fix .",
    "biome:unsafe": "biome check --fix --unsafe .",
    "eslint": "eslint --config eslint.config.mjs --report-unused-disable-directives --max-warnings 0 .",
    "prettier": "prettier --config .prettierrc --write .",
    "backups": "grunt backups",
    "pkg-check": "depcheck",
    "pkg-upgrade": "ncu --interactive --format group --packageManager bun",
    "versioning": "ungit",
    "nvm": "nvm use",
    "script:sass-charset": "cd scripts && bun sass-charset.js",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.97.8",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-cluster": "^2.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.5",
    "@playwright/test": "^1.49.1",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-vite": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@tanstack/router-cli": "^1.97.8",
    "@tanstack/router-plugin": "^1.97.8",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.1.17",
    "@types/eslint": "9.6.1",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.16",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "commitlint-config-gitmoji": "^2.3.1",
    "depcheck": "^1.4.7",
    "eslint": "9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "grunt-cli": "^1.5.0",
    "grunt-contrib-compress": "^2.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "load-grunt-tasks": "^5.1.0",
    "npm-check-updates": "^17.1.14",
    "playwright": "^1.49.1",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "rimraf": "^6.0.1",
    "sass": "1.83.4",
    "storybook": "^8.5.1",
    "tailwindcss": "3.4.17",
    "ts-jest": "^29.2.5",
    "typescript": "5.7.3",
    "ungit": "^1.5.28",
    "vite": "6.0.9",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
